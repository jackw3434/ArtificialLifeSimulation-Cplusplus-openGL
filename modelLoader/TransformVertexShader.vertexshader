#version 400 core

layout(location = 0) in vec3 vertexPosition_modelspace;
layout(location = 1) in vec2 vertexUV;

out vec2 UV;

uniform vec3 lightPos;

uniform vec4 ambient;
uniform vec3 dLight;

uniform vec3 sLight;
uniform float sShine;

uniform mat4 MVP;

void main(){
  gl_Position = MVP * vec4(vertexPosition_modelspace, 1);

  UV = vertexUV;

}
